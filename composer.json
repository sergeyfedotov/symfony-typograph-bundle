{
    "name": "fsv/typography-bundle",
    "description": "Symfony typographer",
    "keywords": [
        "symfony2",
        "typograph",
        "mdash"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Fedotov",
            "email": "sergey89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/symfony": "^2.7|^3.0",
        "michelf/php-smartypants": "1.6.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "emuravjev/mdash": "3.5",
        "squizlabs/php_codesniffer": "^2.6"
    },
    "autoload": {
        "psr-4": {
            "Fsv\\TypographyBundle\\": ""
        },
        "classmap": [
            "./vendor/emuravjev/mdash/EMT.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "emuravjev/mdash",
                "version": "3.5",
                "dist": {
                    "url": "http://mdash.ru/download/emt3.5-php.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/emuravjev/mdash",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["EMT.php"]
                }
            }
        }
    ],
    "config": {
        "secure-http": false
    }
}
