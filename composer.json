{
    "name": "fsv/typography-bundle",
    "description": "Symfony2 typograph for Russian Language",
    "keywords": [
        "symfony2",
        "typograph",
        "mdash"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Fedotov",
            "email": "sergey89@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.9",
        "symfony/symfony": "~2.7",
        "emuravjev/mdash": "3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8"
    },
    "autoload": {
        "psr-4": {
            "Fsv\\TypographyBundle\\": ""
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "emuravjev/mdash",
                "version": "3.5",
                "dist": {
                    "url": "http://mdash.ru/download/emt3.5-php.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/emuravjev/mdash",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "classmap": ["EMT.php"]
                }
            }
        }
    ]
}
